@using PierreTracker.Models;

<h3>Here are all the orders for this vendor:</h3>

<ol>
@foreach (Vendor vendor in @Model["vendors"])
{
  <li><a href='/vendors/@Model["vendor"].Id/vendors/@vendor.Id'>@vendor.Name</a></li>
}
</ol>

<p><a href='/vendors/@Model["vendor"].Id/vendors/new'>Add a new order</a></p>
<p><a href='/vendors'>Return to vendor list</a></p>
<p><a href='/'>Return to Main Page</a></p>